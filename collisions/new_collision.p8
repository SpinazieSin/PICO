pico-8 cartridge // http://www.pico-8.com
version 18
__lua__


function _init()
	x = 50
	y = 50
end

function _update()
	if btn(0) then
		x+=1
		y+=1
	end
	if btn(1) then
		x+=1
		y-=1
	end
	if btn(2) then
		x-=1
		y+=1
	end
	if btn(3) then
		x-=1
		y-=1
	end
end

function _draw()
	cls()
	rectfill(0, 0, 10, 127, 14)
end

